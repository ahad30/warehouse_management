#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/komor/volume 2/projects/others/Invoice-Generator-System/Frontend/node_modules/browserslist/node_modules:/media/komor/volume 2/projects/others/Invoice-Generator-System/Frontend/node_modules:/media/komor/volume 2/projects/others/Invoice-Generator-System/node_modules:/media/komor/volume 2/projects/others/node_modules:/media/komor/volume 2/projects/node_modules:/media/komor/volume 2/node_modules:/media/komor/node_modules:/media/node_modules:/node_modules:/media/komor/volume 2/projects/others/Invoice-Generator-System/Frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/komor/volume 2/projects/others/Invoice-Generator-System/Frontend/node_modules/browserslist/node_modules:/media/komor/volume 2/projects/others/Invoice-Generator-System/Frontend/node_modules:/media/komor/volume 2/projects/others/Invoice-Generator-System/node_modules:/media/komor/volume 2/projects/others/node_modules:/media/komor/volume 2/projects/node_modules:/media/komor/volume 2/node_modules:/media/komor/node_modules:/media/node_modules:/node_modules:/media/komor/volume 2/projects/others/Invoice-Generator-System/Frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browserslist/cli.js" "$@"
else
  exec node  "$basedir/../browserslist/cli.js" "$@"
fi
